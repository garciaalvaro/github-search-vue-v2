{
	"name": "github-search-vue-v2",
	"version": "0.0.1",
	"description": "Small app to search GitHub repositories",
	"scripts": {
		"start": "npm run dev",
		"prebuild": "npm run clean:dist && npm run test:types",
		"build": "webpack --mode=production",
		"clean:dist": "rm -rf dist",
		"predev": "npm run clean:dist",
		"dev": "webpack --mode=development & npm run serve:dev",
		"test": "npm run test:types && npm run test:lint && npm run test:prettier",
		"test:types": "tsc",
		"test:lint": "eslint \"**/*.{js,ts,vue}\"",
		"test:prettier": "prettier --check \"**/*.{js,ts,vue,json,html}\"",
		"prettier": "prettier --write \"**/*.{js,ts,vue,json,html}\"",
		"test:unit": "jest",
		"test:unit:watch": "jest --watch",
		"docker": "docker-compose up -d",
		"docker:recreate": "docker-compose down && docker-compose build --no-cache && docker-compose up",
		"docker:start": "npm run build && npm run serve:docker",
		"docker:stop": "docker-compose down",
		"serve": "pm2 start server.js --name github-search-vue-v2",
		"serve:stop": "pm2 stop github-search-vue-v2",
		"serve:dev": "pm2-dev start server.js",
		"serve:docker": "pm2-runtime start server.js --name github-search-vue-v2"
	},
	"dependencies": {
		"@babel/core": "^7.12.7",
		"@babel/plugin-transform-runtime": "^7.12.1",
		"@babel/preset-env": "^7.12.7",
		"@babel/preset-typescript": "^7.12.7",
		"@babel/register": "^7.12.1",
		"@babel/runtime": "^7.12.5",
		"@types/jest": "^26.0.15",
		"babel-loader": "^8.2.1",
		"css-loader": "^5.0.1",
		"css-minimizer-webpack-plugin": "^1.1.5",
		"express": "^4.17.1",
		"html-webpack-plugin": "^4.5.0",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^26.6.3",
		"mini-css-extract-plugin": "^1.3.1",
		"nib": "^1.1.2",
		"pm2": "^4.5.0",
		"postcss-loader": "^4.1.0",
		"postcss-preset-env": "^6.7.0",
		"stylus": "^0.54.8",
		"stylus-loader": "^4.3.0",
		"terser-webpack-plugin": "^5.0.3",
		"typescript": "^4.1.2",
		"vue": "^2.6.12",
		"vue-loader": "^15.9.5",
		"vue-template-compiler": "^2.6.12",
		"webpack": "^5.6.0",
		"webpack-cli": "^4.2.0"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^4.8.2",
		"@typescript-eslint/parser": "^4.8.2",
		"@vue/eslint-config-typescript": "^7.0.0",
		"eslint": "^7.14.0",
		"eslint-config-prettier": "^6.15.0",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-vue": "^7.1.0",
		"prettier": "^2.2.0",
		"vue-eslint-parser": "^7.1.1"
	},
	"main": "server.js",
	"author": "Alvaro Garcia",
	"license": "MIT",
	"private": true
}
